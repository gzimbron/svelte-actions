{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", "build/**"]
		},
		"lint": {},
		"test": {},
		"test:ui": {},
		"test:watch": {},
		"package": {
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"dev-docs": {
			"cache": false,
			"persistent": true
		}
	}
}
